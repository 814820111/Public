            $('.Unbind').css('display', 'block');
        }

        function Unhidd() {
            $('.Unbind').css('display', 'none');
        }
        function Unhidden() {

            $('.Unbind').css('display', 'none');
            $.ajax({
                url: "{:U('ChangeSession/Unbind')}",
                type: "post",
                dataType: "json",
                data: {
                   state:1
                },
                success: function(res) {
                    if(res.status=="success"){
                        alert("取消成功");
                        WeixinJSBridge.call('closeWindow');
                    }

                    if(res.status=="error"){
                        alert("取消失败");
                    }


