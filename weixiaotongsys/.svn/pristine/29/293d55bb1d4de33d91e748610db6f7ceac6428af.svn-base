*{
	margin: 0;padding: 0;
	font-family:"微软雅黑 Light"; 
	}
.clearfix{clear: both;}
body{
	background-color:#eff0f2; 
	padding-bottom: 30px;
}
/*内容区*/
.bigbox{ 
	margin:8px 10px 0px;
	background-color:white;
	border-radius:5px;
}
.teacher{ border-bottom:#d2d2d2 dashed 1px; padding:10px 10px;}
.teacher img{ float:left; margin-right:7px; width:40px;}
.teacher .left{ float:left; padding-top:3px;}
.teacher .right{
	float:right;color:#979995; 
	font-size:12.5px; 
	padding-top:3px;
}
.teacher .left .top{color:#0d0d0d; 
	font-size:13.5px;
	font-weight:bold;
}
.teacher .left .bot{color:#979995; font-size:12.5px;}
/*文字*/
.wenzi .one{ font-size:14.5px; color:#0d0d0d; 
	text-align:center; margin-top:15px; 
	font-family:"微软雅黑 Light"; 
	font-weight:bold;
}
.wenzi .two{ font-size:13.5px; color:#262626; padding:6px 10px 10px;}
/*图片*/
.bigbox .photo{
	padding:0px 10px 15px 10px;
	border-bottom:#d2d2d2 dashed 1px;
}
.bigbox .photo img{ width:100%;}
/*按钮*/
.bigbox .but{padding:10px 10px 15px;
	font-weight:bold;
}
.bigbox .but .left{font-size:13.5px; float:left; color:#ffb94a;}
.bigbox .but .right{ font-size:13.5px; float:right; color:#06c18e;}
/*活动时间*/
.big{ 
	font-weight:bold;
	background-color:#FFF; 
	margin-top:8px; 
	color:#636363;
}
.big .box{ 
	padding-left:10px; 
	padding-right:10px;
	border-bottom:#d2d2d2 solid 1px;
}
.big .boxno{padding-left:10px; padding-right:10px;}
.big .left{ line-height:50px; font-size:14px; float:left;}
.big .right{ line-height:50px; font-size:14px; float:right;}