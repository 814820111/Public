*{
	font-family: "微软雅黑 Light";
	margin: 0;
	padding: 0;
}
.clearfix{
	clear: both;
}

.container{
	padding: 2% 2.5% 15%;
	background-color: rgb(245,245,245);
	/*background-color: #f5f5f5;*/
}
.climb,.shuimu,.seaside,.seaside2{
	padding:3% 3% 1%;
	background-color: white;
	border-radius: 5px;
}
a{
	text-decoration: none;color: black;
}
.climb :hover{
	cursor: pointer;

}
.shuimu,.seaside2,.climb{
	margin-top: 8px;
}
.one{
	font-size: 14.5px;
	font-weight: 600;
	margin-top: 7px;
	text-align: center;
	margin-bottom: 4px;
}
.two{
	margin-bottom: 8px;
}
.two span{
	font-size: 14px;
}

.two .img{
	padding-left: 15px;
}
.img3{
	margin-top: 6px;
	width: 25%;
}
.img2{
	margin-top: 6px;
	width: 70%;
}
.three{
	margin-top: 6px;
	margin-bottom: 8px;
}
.left{
	width: 45%;
	float: left;
}
.left img{
	display: block;
	margin-top: -2px;
	float: left;
	width: 25px;
}
.right{
	font-weight: bold;
	font-size: 12.5px;
	float: right;
}
.three .left span{
	display: block;
	/* margin-left: 23px; */
	margin-top: 2px;
	font-size: 13.5px;
	/* color: rgb(151,153,150); */
	/* font-weight: bold; */
}
.three .right{
	font-size: 10px;
	margin-top: 3px;
}
.three .right span{
	color: rgb(151,153,150);
}
.pandect{
	margin-top: 2%;
	margin-bottom: 3%;
	font-size: 14px;
}
.pandect .renshu{
	float: left;
	color: rgb(255,184,73);
}
.pandect .baoming{
	float: right;
	color: #06c18e;
}
.pandect .baoming1{
	float: right;
	color: rgb(255,184,73);
}

.divide{
	border: none;
	border-top: 1px dashed rgb(239,239,239);
}