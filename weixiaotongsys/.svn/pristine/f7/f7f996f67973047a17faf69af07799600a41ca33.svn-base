<body style="background-color: #eff0f2;">
    <input type="hidden" value={$userid} id="userid">
    <div class="top_box">
        <a href="__ROOT__/index.php/weixin/Tindex"><img src="__TMPL__Public/images/img674.png"></a>
        成长档案
        <div class="clearfix"></div>
    </div>
    <div class="container" style=" overflow:hidden;margin-top: 26px;">

     <foreach name="alist" item="v">
         <a href="{:U('details',array('aid'=>$v['archivesid'],'gid'=>$v['gradeid']))}">
         <div class="wednesday">
           <div class="two" style="float:left">
                <div >
                  <span>档案&nbsp;：&nbsp;{$v.aname}</span>
                </div>
                <div>
                   <span>书名&nbsp;：&nbsp;{$v.bookname}</span>
                </div>
               <div>
                   <span>年级&nbsp;：&nbsp;{$v.gname}</span>
               </div>
               <div>
                   <span>学期&nbsp;：&nbsp;{$v.semester}</span>
                </div>
           </div>
             <div class="four" style="float:right">
                 <img  style="height:100px;width:80px;" src="{$Think.const.GSRC}51.jpg">
             </div>

             <div class="clearfix"></div>
             <hr class="divide">
          <div class="three" >
               <div class="left">
                 <img  src="__TMPL__Public/images/feiji.png" alt="">
                   <a href="{:U('classList',array('aid'=>$v['archivesid'],'gid'=>$v['gradeid']))}">
                 <span style="font-size: 12.5px;color:red;">家长进度</span>
                   </a>
                 </div>
                <div class="right">
                   <span>{$v.create_time|date='Y-m-d H:i:s',###}</span>
                  </div>
                <div class="clearfix"></div>
          </div>
        </div>
         </a>
     </foreach>

    </div>

