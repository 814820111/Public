<admintpl file="header" />
<body class="J_scroll_fixed">
<div class="wrap jj">
    <ul class="nav nav-tabs">
        <li><a href="{:U('device_index')}">设备列表</a></li>
        <li class="active"><a href="{:U('add_device')}">添加设备</a></li>
    </ul>
    <div class="common-form">
        <form method="post" class="form-horizontal J_ajaxForms" action="{:U('add_device_post')}">
            <fieldset>
                <div class="control-group">
                    <label class="control-label">应用名称:</label>
                    <div class="controls">
                        <select class="input" required name="applicationid" >
                            <option value="">请选择应用 </option>
                            <foreach name="application" item="vo">
                                <option value="{$vo.id}">{$vo.name}</option>
                            </foreach>

                        </select>

                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label">设备序列号:</label>
                    <div class="controls">
                        <input type="text" required  class="input input_hd J_title_color"   name="deviceKey" value="">
                    </div>
                </div>

                <div class="control-group">
                    <label class="control-label">设备名称:</label>
                    <div class="controls">
                        <input type="text" required class="input" name="name" value="">
                    </div>
                </div>


            </fieldset>
            <div class="form-actions">
                <button type="submit" class="btn btn-primary btn_submit J_ajax_submit_btn">提交</button>
                <button class="btn" type="reset">清空</button>
            </div>
        </form>
    </div>
</div>
<script src="__ROOT__/statics/js/common.js"></script>
<script>

	
</script>
</body>
</html>