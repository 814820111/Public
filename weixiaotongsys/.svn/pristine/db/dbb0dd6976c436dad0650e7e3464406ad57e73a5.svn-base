.wrap{ width: 100%; background:#f8f8f8; background-image:url(../images/Bg_Pic.jpg); font-family:"微软雅黑";}
.header{ position:relative; width:100%; height:100px; background:#e8f3f7; font-family:"微软雅黑";}
.head{ width:1100px; height:100px; margin:0 auto;}
.header a.logo{ display:block; float:left; padding-top:14px;}
.header ul{ float:right; margin-top:30px;}
.header ul li{ float:left; padding:6px 0; width:auto; text-align:center;}
.header ul li a{ display:block; float:left; font-size:18px; color:#595757; padding-bottom:8px;}
.header ul li a:hover{ color:#4889c9;}
.header ul li:hover a{ border-bottom:3px solid #4889c9;}
.header ul li.active a{ border-bottom:3px solid #4889c9;}
.header ul li.active a{ color:#4889c9;}
.header ul em{ float:left; font-style:inherit; font-size:18px; color:#b5b5b6; padding:0px 16px;}

.banner{ width:100%; background:#f8f8f8; position:relative;}
.banner_m{ width:1000px; margin:0 auto;}

.footer{ position:relative; width:100%; background:#748186; font-family:"微软雅黑";}
.footer .foot{ width:1000px; margin:0 auto;}
.footer .foot p{ text-align:center; color:#afd5f9; font-size:15px;}
.footer .foot_link{ position:relative; width:100%; overflow:hidden; border-bottom:1px solid #85bbe7; padding-bottom:20px; margin-bottom:16px;}
.footer .foot_link a{ display:block; position:absolute;}
.footer .foot_link a.twlink{ left:100px; top:0px;}
.footer .foot_link a.twtel{ left:260px; top 0px;}
.footer .foot_link p{ color:#eff6fb; text-align:left; padding-left:430px; line-height:24px;}


.h20{ padding-bottom:20px;}
.index_banner{ background:url(../images/bannerbg.jpg) repeat-x; width:100%; height:490px;}
.index_banner_m{ width:1000px; height:470px; position:relative; margin:0 auto;}
.index_banner_img{ position:absolute;}
.index_banner_img001{ left:0px; top:100px;}
.index_banner_img002{ left:0px; bottom:80px;}
.index_banner_img003{ left:200px; bottom:80px;}
.index_banner_img004{ right:10px; bottom:40px;}
.index_banner_m_btn{ text-align:center; margin:0 auto;}
.index_banner_m_btn li{ float:left; width:50px; height:4px; background:#e3f0fc; margin:0 3px; cursor:pointer;}
.index_banner_m_btn li.active{ background:#207ad3;}
.index_banner_m ul li{ display:none;}
