<div style="margin-left: 25px;margin-right: 20px;margin-top: 30px;">
		

		<form action="{:u('InfoTemplateSet/index_special_add_post')}" method="post" class="form-horizontal js-ajax-forms" >
			<div class="table_list">
				<table class="table table-hover table-bordered">
					<tr>
						<th>活动名称:</th>
						<td style=" padding: 0;"><input type="text" name="event_name" id="event_name" value="" style="width: 100%; height: 40px; border: none;" placeholder="请输入:"></td>
					</tr>

					<tr>
						<th>开始时间:</th>
						<td style=" padding: 0;"><input class="sang_Calender" type="text" name="start_time" id="start_time" value="" style="width: 100%; height: 40px; border: none;" placeholder="请输入:"></td>
					</tr>

					<tr>
						<th>结束时间:</th>
						<td style=" padding: 0;"><input class="sang_Calender" type="text" name="end_time" id="end_time" value="" style="width: 100%; height: 40px; border: none;" placeholder="请输入:"></td>
					</tr>

					<tr>
						<th>内容详情:</th>
						<td style=" padding: 0;"><input type="text" name="content" id="content" value="" style="width: 100%; height: 40px; border: none;" placeholder="请输入:"></td>
					</tr>

					<tr>
						<th>开始/关闭：</th>
						 <td>
						  <select class="select_2" name="type">
					        <option value="1">开启</option>
					        <option value="2">关闭</option>
					      </select> &nbsp;&nbsp;
					     </td>
							
					</tr>


				</table>
			</div>

			  <div >
                  <button type="submit" class="btn btn-primary btn_submit J_ajax_submit_btn" style="border:none;;color:#FFF; background-color:#26a69a; margin-right:10%;"> &nbsp;发  &nbsp;送&nbsp; </button>
                  <button type="reset" class="btn btn-default" style="border:none;;color:#FFF; background-color:#adadad ;margin-right:10%;"> &nbsp;重 &nbsp;置&nbsp; </button>
				  <button type="rel" class="btn btn-default" style="border:none;;color:#FFF; background-color:#adadad"> &nbsp;返 &nbsp;回&nbsp; </button>
              </div>
			
		</form>
</div>


