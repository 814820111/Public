@charset "utf-8";
/* CSS Document */
*{margin:0; padding:0;font-family:"微软雅黑"}
a:hover{ text-decoration:none;}
ul{ margin:0;}
li{ list-style:none;}
clearfix{ clear:both;}
/*banner*/
.banner{position:relative;}
.over{ overflow:hidden;}
.class-box{ position:absolute; border:1px solid rgba(0,0,0,0.00);}
.jingguo{ background-color:rgba(155,252,217,1.00);}
.class-box div{ background-color:white;}
.lbox:hover{ background-color:rgba(160,248,238,1.00); cursor:pointer;}
/*one*/
.one ul li{ display:block; width:82px; margin:0 auto;}
/*huitiao*/
.huitiao3{ width:100%; height:2px; background-color:#eceef0;}
/*tubiao*/
.tubiao{float:left;width: 100%;}
.tubiao div{ float:left;width: 18%; text-align: center; margin-left: 1%; margin-right: 1%;}
.tubiao img{ width:80%;}
/*top*/
.top .left{ float:left; width:50%; border-right:1px solid #e3e3e3;}
.top .right{ float:right; width:50%; }
.top .zuo{ float:left; margin-top:8%; margin-left:1%}
