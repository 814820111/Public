    <script>
        $("document").ready(function() {
            var classid = $("#classid").val();
            var schoolid = $("#schoolid").val();
            $.ajax({
                type: "post",
                url: '__ROOT__/index.php/?g=apps&m=school&a=ClassSyllabus',
                async: true,
                data: {
                    schoolid: schoolid,

                    classid: classid
                },
                dataType: 'json',
                success: function(res) {
                    console.log(res);
                    res = eval(res.data);
                    var mon = res.mon;
                    for (var i = 0; i < mon.length; i++) {
                        var jian = mon[i][i + 1];
                        var j = "a" + i;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }
                    var tu = res.tu;
                    for (var g = 0; g < tu.length; g++) {
                        var jian = tu[g][g + 1];
                        var j = "k" + g;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }
                    var tu = res.we;
                    for (var g = 0; g < tu.length; g++) {
                        var jian = tu[g][g + 1];
                        var j = "f" + g;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }

                    var tu = res.th;
                    for (var g = 0; g < tu.length; g++) {
                        var jian = tu[g][g + 1];
                        var j = "h" + g;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }


                    var tu = res.fri;
                    for (var g = 0; g < tu.length; g++) {
                        var jian = tu[g][g + 1];
                        var j = "y" + g;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }


                    var tu = res.sat;
                    for (var g = 0; g < tu.length; g++) {
                        var jian = tu[g][g + 1];
                        var j = "o" + g;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }

                    var tu = res.sun;

                    for (var g = 0; g < tu.length; g++) {
                        var jian = tu[g][g + 1];

                        var j = "x" + g;

                        var test = document.getElementsByClassName(j);
                        $(test).append(jian);

                    }


                },
                error: function() {

                    alert('系统错误,请稍后重试');
                }
            });
        })

        $(".fanhui").click(function() {
            location.href = "{:U('Index/index')}";
        })
    </script>
