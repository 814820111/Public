*{margin: 0;padding: 0;}
.clearfix{clear: both;}
body{background-color:#eff0f2;font-family:"微软雅黑 Light";}

/*内容区*/
.bigbox{ margin:8px 10px 0px;
	background-color:white;border-radius:5px;
}
.teacher{ border-bottom:#d2d2d2 dashed 1px; padding:10px 10px;}
.teacher img{ float:left; margin-right:7px; width:40px;}
.teacher .left{ float:left; padding-top:3px;}
.teacher .right{ float:right;color:#979995; 
	font-size:12.5px; padding-top:3px;
}
.teacher .left .top{
	color:#0d0d0d; font-size:13.5px;
	font-weight: 600;
}
.teacher .left .bot{color:#979995; font-size:11.5px;}
/*文字*/
h4{ text-align:center; 
	margin-top:10px;
	font-size: 14.5px;
}
.wenzi{ font-size:13.5px; color:#262626; padding:10px 10px;}
/*图片*/
.bigbox .photo{padding:0px 10px 15px 10px;}

/*提示*/
.tishi{ font-size:15px; color:#ffb94a; padding:15px 0 15px 15px;}
.img2{
	text-align: center;
}
.img3{
	margin-top: 6px;
	width: 25%;
}
.img2{
	margin-top: 6px;
	width: 70%;
}


.img2{
	margin-left: 1.5%;
	margin-right: 1.5%;
}

