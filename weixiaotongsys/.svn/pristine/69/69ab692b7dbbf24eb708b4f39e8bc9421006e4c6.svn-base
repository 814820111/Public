*{
	margin: 0;
	padding: 0;
	font-family: "微软雅黑 Light";
}
.clearfix{
	clear: both;
}
body{
	background-color: rgb(239,240,243);
}
.container{
	margin-left: 2.5%;
	margin-right: 2.4%;
}
.fenlei{
	padding-top: 8px;
	padding-bottom: 12px;
}
.fenlei .shenghuo,.qinshu,.yuyin{
	font-size: 14px;
	width: 33.1%;
	padding-bottom: 8px;
	padding-top: 8px;
	float: left;
	border-top: 1px solid rgb(6,193,142);
	border-bottom: 1px solid rgb(6,193,142);
	text-align: center;
}
.fenlei .yuyin{
	border-left: 1px solid rgb(6,193,142);
	border-right: 1px solid rgb(6,193,142);
}
.fenlei .shenghuo{
	background-color: rgb(6,193,142);
	color: white;
}
.fenlei .qinshu,.yuyin{
	background-color: white;
	color: rgb(6,193,142);
}
.tishi{
	text-align: center;
	font-size: 12.5px;
	margin-bottom: 12px;
	color: rgb(130,130,130);
}
.addphoto{
	background-color: white;
	height: 250px;
}
.addphoto .add{
	padding-top: 40px;
}
.addphoto .add .border{
	margin-left: auto;
	margin-right: auto;
	border: 2px dashed rgb(151,153,149);
	height: 120px;
	width: 120px;
}
.addphoto .add .border .heng{
	width: 50%;
	border-top: 2px solid rgb(151,153,149);
	margin-left: auto;
	margin-right: auto;
	margin-top: 60px;
}
.addphoto .add .border .shu{
	margin-left: 49%;
	margin-top: -31px;
	height: 60px;
	border-left: 2px solid rgb(151,153,149);
}
.addphoto .shangchuan{
	margin-top: 15px;
	text-align: center;
	font-size: 13.5px;
	font-weight: bold;
}
.liuyan{
	width: 100%;	
	height: 60px;
	padding-top: 7px;
	resize:none;
	border: none;
	color: black;
}
.button{
	margin-top: 23px;
	color: white;
	font-size: 14px;
}
.button .cancel{
	padding: 8px 37px;
	margin-left: 8%;
	border-radius: 5px;
	background-color: rgb(238,158,19);
	float: left;
}
.button .conserve{
	padding: 8px 37px;
	margin-right: 8%;
	border-radius: 5px;
	background-color: rgb(6,193,142);
	float: right;
}