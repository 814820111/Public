.footer {
    height: 75px;
}

.footer .fone {
    float: left;
    width: 37.5%;
    border-top: 1px solid rgb(226, 226, 226);
}

.footer .ftwo {
    float: right;
    width: 37.5%;
    border-top: 1px solid rgb(226, 226, 226);
}

.footer img {
    width: 32px;
}

.footer .f1, .f2, .f4, .f5 {
    width: 50%;
    margin-top: 4px;
}

.footer .f3 {
    margin-left: 6px;
    border-radius: 40px;
    border-top: 2px solid rgb(226, 226, 226);
    margin-top: -29px;
    width: 22.5%;
}

.footer .f3 .circle {
    margin-top: 3px;
    margin-left: auto;
    margin-right: auto;
    width: 90%;
    height: 65px;
    background-color: #06c08e;
    border-radius: 50px;
    position: relative;

}
.footer .f3 .circle .hor{
	width: 69.5%;
	height: 6%;
	background-color: white;
	border-radius: 10px;
	position: absolute;
	top: 47.5%;
	left: 15%;

}
.footer .f3 .circle .ver{
	height: 69.5%;
	width: 4px;
	position: absolute;
	top: 15%;
	left: 47.5%;
	border-radius: 10px;
	background-color: white;
}

.f2 .tongxun {
    font-size: 13px;
    font-family: "微软雅黑" ;
    color : #06c08e;
}

.fword {
    font-size: 13px;
    font-family: "微软雅黑" ;
    color : rgb(81, 81, 81);
}