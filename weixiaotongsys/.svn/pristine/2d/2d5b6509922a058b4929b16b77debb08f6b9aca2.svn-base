}

.search{
    padding: 5px;    
    background-color: rgb(230,242,243);
}
.search .context{
    border: 1px solid rgb(222,222,222);
    border-radius: 5px;
    padding: 8px;
    text-align: center;
    background-color: white;
}
.search .context img{
    margin-top: -3px;
}
.search .context span{
    font-size: 14px;
    font-weight: 600;
    color: rgb(162,163,162);
}
.one{
    margin-bottom: -23px;
}
