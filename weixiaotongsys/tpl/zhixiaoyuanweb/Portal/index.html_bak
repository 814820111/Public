<div class="long_pic_box">
	<div class="long_pic_login">
		<div class="long_pic_login_input">
			<div class="container-fluid">
				<div class="row-fluid">
					<div class="span12">
						<div class="tabbable">
							<ul class="nav nav-tabs">
								<li class="active">
									<a href="#parent-login" data-toggle="tab">家长</a>
								</li>
								<li>
									<a href="#teacher-login" data-toggle="tab">教师</a>
								</li>
							</ul>
							<div class="tab-content">
								<div class="tab-pane active" id="parent-login">
									<div class="login_from">
										<form id="parent_from" action="{:U('User/Login/login_post')}" method="post">
											<div class="login_verification">
												<div class="verification_loading"></div>
												<div class="verification_text">登陆中...</div>
											</div>
											<div class="input_box">
												<div class="user_icon"></div>
												<input id="parent_user" type="text" name="phone" placeholder="账号">
											</div>
											<div class="input_box">
												<div class="pass_icon"></div>
												<input type="password" id="parent_password" name="password" placeholder="密码">
												<input type="hidden" id="parent_type" name="user_type" value="2">
											</div>
											<div class="login_checkbox">
												<input class="in1" type="checkbox" value=""><p class="p1">记住密码</p>
												<input class="in2" type="checkbox" value=""><p class="p2">自动登录</p>
												<button type="submit" class="btn btn-success" value="登录">登录</button>
											</div>

										</form>
									</div>
								</div>
								<div class="tab-pane" id="teacher-login">
									<div class="login_from">
										<form id="teacher_from" action="{:U('User/Login/login_post')}" method="post">
											<div class="login_verification">
												<div class="verification_loading"></div>
												<div class="verification_text">登陆中...</div>
											</div>
											<div class="input_box">
												<div class="user_icon"></div>
												<input id="teacher_user" type="text" name="phone" placeholder="账号">
											</div>
											<div class="input_box">
												<div class="pass_icon"></div>
												<input type="password" id="teacher_password" name="password" placeholder="密码">
												<input type="hidden" id="teacher_type" name="user_type" value="1">
											</div>
											<div class="login_checkbox">
												<input class="in1" type="checkbox" value=""><p class="p1">记住密码</p>
												<input class="in2" type="checkbox" value=""><p class="p2">自动登录</p>
												<button type="submit" value="登录">登录</button>
											</div>

										</form>
									</div>
